local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Nosta pelaaja ilmaan
local bodyVelocity = Instance.new("BodyVelocity")
bodyVelocity.MaxForce = Vector3.new(400000, 400000, 400000) -- Varmistaa, että liike ei ole rajoitettu
bodyVelocity.Velocity = Vector3.new(0, 50, 0) -- Nousee ylös
bodyVelocity.Parent = character:WaitForChild("HumanoidRootPart")

-- Poista BodyVelocity, jotta se ei jatkuvasti nosta ilmaan
wait(2) -- Odotetaan 2 sekuntia ennen poistoa
bodyVelocity:Destroy()
